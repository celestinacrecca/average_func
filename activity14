#include<stdio.h>

int sub1();
int sub2();
int sub3();
int sub4();
int sub5();

int main(){

    int math, science, english, philosophy, cookery;

    math = sub1();
    science = sub2();
    english = sub3();
    philosophy = sub4();
    cookery = sub5();

    printf("%d plus %d plus %d plus %d plus %d is %d ", math, science, english, philosophy, cookery, math+science+english+philosophy+cookery);
    printf("The average is %d", (math+ science+english+philosophy+cookery)/5);

    return 0;
}

int sub1(){

    int n1;
    printf("Enter the grade: ");
    scanf("%d", &n1);

    return n1;
}

